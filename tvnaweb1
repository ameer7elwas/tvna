        async function loadDashboardData() {
            try {
                // Ø¥Ø­ØµØ§Ø¦ÙŠØ§Øª Ø§Ù„Ù‚Ù†ÙˆØ§Øª
                const { count: channels, error: channelsError } = await supabase
                    .from('channels')
                    .select('*', { count: 'exact' })
                    .eq('is_active', true);
                
                if (!channelsError) channelsCount.textContent = channels;
                
                // Ø¥Ø­ØµØ§Ø¦ÙŠØ§Øª Ø§Ù„Ù…Ø´Ø§Ù‡Ø¯Ø§Øª
                const today = new Date();
                today.setHours(0, 0, 0, 0);
                
                const { count: views, error: viewsError } = await supabase
                    .from('view_stats')
                    .select('*', { count: 'exact' })
                    .gte('created_at', today.toISOString());
                
                if (!viewsError) viewsCount.textContent = views;
                
                // Ø¥Ø­ØµØ§Ø¦ÙŠØ§Øª Ø§Ù„Ù…Ø³ØªØ®Ø¯Ù…ÙŠÙ†
                const { count: users, error: usersError } = await supabase
                    .from('users')
                    .select('*', { count: 'exact' })
                    .eq('is_active', true);
                
                if (!usersError) usersCount.textContent = users;
                
            } catch (error) {
                console.error('Error loading dashboard data:', error);
            }
        }
        
